<div class="container">
  <div class="row">
    <div class="col-md-7 pt-3 offset-md-1 mt-3">
      <h5 class="mb-3">予約一覧</h5>
      <table class="table mb-5">
        <tr>
          <th>日付</th>
          <th>時間</th>
          <th>名前</th>
          <th>人数</th>
          <th>電話番号</th>
        </tr>
        <% @reservations.each do |reservation| %>
          <tr>
            <td><%= reservation.date.strftime("%Y/%m/%d") %></td>
            <td><%= reservation.time.strftime("%H:%M") %></td>
            <td><%= reservation.user.last_name %>　<%= reservation.user.first_name %></td>
            <td><%= reservation.people_count %>人</td>
            <td><%= reservation.user.phone_number %></td>
          </tr>
        <% end %>
      </table>
    </div>
    </div>
  </div>
</div>